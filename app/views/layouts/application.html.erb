<!DOCTYPE html>
<html>
  <head>
    <%= render "shared/head"%>
    <%= favicon_link_tag %>
    <%= render "shared/analytics"%>
  </head>

  <body class="animsition <%= controller.controller_name %>-<%= controller.action_name %>">
    <%= render "shared/facebook"%>
    <%= render "shared/navbar"%>
    <%= yield %>
    <%= render "shared/footer"%>
    <%= javascript_include_tag "https://maps.google.com/maps/api/js?sensor=false&libraries=places&key=AIzaSyAnInNCmJEkZBxn_qwwNPyIt_ZnZGB7GrU" %>
    <%= javascript_include_tag "https://google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js" %>
    <%= javascript_include_tag "application" %>
    <%= yield(:js) %>
  </body>
</html>


